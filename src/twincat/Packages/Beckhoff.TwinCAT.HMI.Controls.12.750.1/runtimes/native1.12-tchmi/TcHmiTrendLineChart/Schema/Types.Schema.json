                {
                    "name": "labelFontColor",
                    "category": "Colors",
                    "displayName": "Label font color",
                    "displayPriority": 10,
                    "description": "",
                    "defaultValue": null,
                    "defaultValueInternal": {
                        "color": "#4794da"
                    }
                },
                {
                    "name": "axisColor",
                    "category": "Colors",
                    "displayName": "Axis color",
